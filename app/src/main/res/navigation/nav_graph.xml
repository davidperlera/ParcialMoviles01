<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/bildboardFragment">

    <fragment
        android:id="@+id/bildboardFragment"
        android:name="com.perlera.parcial01.ui.food.bildboard.BildboardFragment"
        android:label="fragment_bildboard"
        tools:layout="@layout/fragment_bildboard" >
        <action
            android:id="@+id/action_bildboardFragment_to_newFoodFragment"
            app:destination="@id/newFoodFragment" />
        <action
            android:id="@+id/action_bildboardFragment_to_foodFragment"
            app:destination="@id/foodFragment" />
    </fragment>
    <fragment
        android:id="@+id/newFoodFragment"
        android:name="com.perlera.parcial01.ui.food.newfood.NewFoodFragment"
        android:label="fragment_new_food"
        tools:layout="@layout/fragment_new_food" />
    <fragment
        android:id="@+id/foodFragment"
        android:name="com.perlera.parcial01.ui.food.FoodFragment"
        android:label="fragment_food"
        tools:layout="@layout/fragment_food" />
</navigation>